# Google Gemini API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# Google Cloud Platform Configuration
# For Imagen 3 inpainting (needed for MVP4)
GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
GOOGLE_CLOUD_LOCATION=us-central1

# Google Maps API Configuration (needed for MVP3)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_API_KEY=your_maps_api_key_here

# Plugin Configuration
DEBUG_MODE=false
LOG_LEVEL=INFO

# Output Configuration
OUTPUT_DIR=C:\\Allplan_Output
TEMP_DIR=C:\\Temp\\AllplanRenderAI

# Default Rendering Settings
DEFAULT_RESOLUTION=2K
DEFAULT_USE_PRO_MODEL=false
DEFAULT_STYLE=Modern
DEFAULT_LIGHTING=Noon

# Cost Tracking
ENABLE_COST_TRACKING=true
MAX_MONTHLY_COST_USD=100.0
